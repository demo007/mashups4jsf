&standard_uicomponent_attributes;
<attribute>
	<name>longitude</name>
	<required>true</required>
	<type>java.lang.String</type>
	<description>
		The longitude of the place you wish to point the map to.
	</description>
</attribute>
<attribute>
	<name>latitude</name>
	<required>true</required>
	<type>java.lang.String</type>
	<description>
		the latitude of the place you wish to point the map to.
	</description>
</attribute>
<attribute>
	<name>width</name>
	<required>false</required>
	<type>java.lang.String</type>
	<description>
		The width of the map.
	</description>
</attribute>
<attribute>
	<name>height</name>
	<required>false</required>
	<type>java.lang.String</type>
	<description>
		The height of the map.
	</description>
</attribute>
